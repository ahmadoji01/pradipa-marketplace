<% flash.each do |type, msg| %>
    <%= render partial: 'spree/producer_dashboard/elements/alert', locals: { message: msg, type: type } %>
<% end %>
<div class="row">
    <div class="col-sm-12 col-md-6">
        <%= render partial: 'spree/producer_dashboard/brand_form' %>
    </div>
    <div class="col-sm-12 col-md-6">
        <%= render partial: 'spree/producer_dashboard/account_info_form', locals: { account: @account } %>
    </div>
</div>